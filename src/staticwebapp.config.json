{
    "navigationFallback": {
      "rewrite": "/index.html",
      "exclude": ["/images/*", "/css/*", "/js/*"]
    },
    "routes": [
      {
        "route": "/fonts/*",
        "headers": {
          "Access-Control-Allow-Origin": "*"
        }
      }
    ],
    "globalHeaders": {
      "Access-Control-Allow-Origin": "*",
      "Access-Control-Allow-Methods": "POST, GET, OPTIONS"
    }
  }
{
  "navigationFallback": {
    "rewrite": "/index.html",
    "exclude": ["/images/*", "/js/*"]
  },
  "routes": [
    {
      "route": "/css/*",
      "headers": {
        "cache-control": "public, max-age=31536000, immutable"
      }
    },
    {
      "route": "/fonts/*",
      "headers": {
        "cache-control": "public, max-age=31536000, immutable",
        "Access-Control-Allow-Origin": "https://www.fonda-design.com"
      }
    }
  ]
}
